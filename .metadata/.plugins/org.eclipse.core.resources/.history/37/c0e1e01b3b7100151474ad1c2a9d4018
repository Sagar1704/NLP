package sea.nlp.pos;

public class Context {
	private TagCounter left;
	private TagCounter itself;
	private TagCounter right;

	public TagCounter getLeft() {
		return left;
	}

	public void setLeft(TagCounter left) {
		this.left = left;
	}

	public TagCounter getItself() {
		return itself;
	}

	public void setItself(TagCounter itself) {
		this.itself = itself;
	}

	public TagCounter getRight() {
		return right;
	}

	public void setRight(TagCounter right) {
		this.right = right;
	}

	@Override
	public String toString() {
		return "Context [left=" + left + ", itself=" + itself + ", right=" + right + "]";
	}

	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + ((itself == null) ? 0 : itself.hashCode());
		result = prime * result + ((left == null) ? 0 : left.hashCode());
		result = prime * result + ((right == null) ? 0 : right.hashCode());
		return result;
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Context other = (Context) obj;
		if (itself == null) {
			if (other.itself != null)
				return false;
		} else if (!itself.equals(other.itself))
			return false;
		if (left == null) {
			if (other.left != null)
				return false;
		} else if (!left.equals(other.left))
			return false;
		if (right == null) {
			if (other.right != null)
				return false;
		} else if (!right.equals(other.right))
			return false;
		return true;
	}

}
